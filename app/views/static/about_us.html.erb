<!-- Page content-->
<!-- Breadcrumb-->
<div class="container mt-5 mb-md-4 pt-5">
  <%= render 'shared/breadcrumb' %>
</div>
<!-- Page header-->
<section class="container mb-5 pb-2">
  <div class="row align-items-center justify-content-center">
    <!-- Hero content-->
    <div class="col-lg-4 col-md-5 col-sm-9 order-md-1 order-2 text-md-start text-center">
      <h1 class="mb-4"><%= t('professional_title') %></h1>
      <p class="mb-4 pb-3 fs-lg"><%= t('professional_text') %></p>
      <%= link_to t('contact_us'), contact_us_path, class: "btn btn-lg btn-primary" %>
    </div>
    <!-- Hero carousel-->
    <div class="col-lg-7 col-md-6 offset-md-1 col-12 order-md-2 order-1">
      <div class="tns-carousel-wrapper tns-controls-static tns-nav-outside">
        <div class="tns-carousel-inner" data-carousel-options="{&quot;loop&quot;: true, &quot;gutter&quot;: 16}">
          <div><%= image_tag("https://images.unsplash.com/photo-1560185127-6ed189bf02f4?q=80&w=2670&auto=format&fit=crop&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D", alt: "About Sistach Finques", class: "rounded-3") %></div>
          <div><%= image_tag("https://images.unsplash.com/photo-1601084881623-cdf9a8ea242c?q=80&w=2670&auto=format&fit=crop&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D", alt: "About Sistach Finques", class: "rounded-3") %></div>
          <div><%= image_tag("https://images.unsplash.com/photo-1585914641050-fa9883c4e21c?q=80&w=2667&auto=format&fit=crop&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D", alt: "About Sistach Finques", class: "rounded-3") %></div>
        </div>
      </div>
    </div>
  </div>
</section>
