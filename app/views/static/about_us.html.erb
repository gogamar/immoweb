            <p class="card-text"><%= Service.last.send("summary_#{locale}") %></p>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
<!-- Find property-->
<section class="container mb-5 pb-sm-3 pb-lg-4">
  <div class="bg-secondary rounded-3">
    <div class="col-md-11 col-12 offset-md-1 p-md-0 p-2 d-flex align-items-center justify-content-between">
      <div class="me-md-5 py-md-5 px-md-0 p-4" style="max-width: 526px;">
        <h2 class="mb-md-4">
          <%= t('buy_with_confidence') %> <br>
          <%= t('sell_without_pressure') %>
        </h2>
        <p class="mb-4 pb-md-3 fs-lg"><%= t('appraisal_explanation') %></p>
        <%= link_to contact_us_path, class: "btn btn-lg btn-primary" do %>
          <i class="fi-search me-2"></i><%= t('contact_us') %>
        <% end %>
      </div>
      <div class="col-4 d-md-block d-none align-self-end px-0">
        <%= image_tag "real-estate/about/01.png", width: "406" %>
      </div>
    </div>
  </div>
</section>
